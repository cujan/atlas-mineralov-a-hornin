
{block content}


<fieldset>
    <legend>Štiepateľnosť</legend>
{control vlozVlastnostForm}
</fieldset>
<table>
    <thead>
    <tr>
        <th>Vlastnosť</th>
        
    </tr>
    </thead>
    <tbody>
        
    {foreach $tasks as $task}
    <tr>
	
        <td>{$iterator->getCounter()} </td>
        <td>{$task->vlastnost}</td>
	<td>
		<a n:href="delete, $task->id">Delete</a>
	</td>
    </tr>
    
    {/foreach}
    
    </tbody>
</table>